		self saveBackup.
		delay wait ]! !
!SystemCategoryAutoSaver methodsFor: 'start/stop' stamp: 'HAW 7/22/2022 15:42:44' prior: 50623221!
start
	
	activated := true.
	
	[self saveLoop] forkAt: Processor userBackgroundPriority named: categoryName, ' autosave'
